{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": ["@opstd/tsconfig/base.json"],
	"compilerOptions": {
		"outDir": "./dist",
		"module": "ESNext",
		"moduleResolution": "node",
		"allowImportingTsExtensions": true,
		"allowJs": true,
		"resolveJsonModule": true
	},
	"include": ["src/**/*"]
}
